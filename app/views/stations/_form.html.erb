<%= simple_form_for @station, :url => stations_path  do |f| %>
  <%= render 'error_messages', object: f.object %>
  <%= f.input :name %>
  <%= f.input :email %>
  <%= f.input :phone %>
  <%= f.input :contract, collection: @station.people_collection %>
  <%= f.input :comment %>
  <%= f.input :terms_of_service, as: :boolean%>
  <%= f.button :submit, "Submit" %>
<% end %>
